import java.io.Serializable;

public class Mesaj implements Serializable{
	    /**
	 * 
	 */
	private static final long serialVersionUID = 26042021L;
		private String text;
	    public Mesaj(String text) {
	        this.text = text;
	    }
		public String getText() {
			return text;
		}
		public void setText(String text) {
			this.text = text;
		}
	    
	}